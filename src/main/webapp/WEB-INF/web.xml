<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controlador.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Parqueo_c</servlet-name>
        <servlet-class>Controlador.Parqueo_c</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Vehiculo_c</servlet-name>
        <servlet-class>Controlador.Vehiculo_c</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registro_c</servlet-name>
        <servlet-class>Controlador.Registro_c</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Salida_c</servlet-name>
        <servlet-class>Controlador.Salida_c</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Empresa_c</servlet-name>
        <servlet-class>Controlador.Empresa_c</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Parqueo_c</servlet-name>
        <url-pattern>/Parqueo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Vehiculo_c</servlet-name>
        <url-pattern>/Vehiculo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro_c</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Salida_c</servlet-name>
        <url-pattern>/Salida</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Empresa_c</servlet-name>
        <url-pattern>/Empresa</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
